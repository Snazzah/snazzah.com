{
  "private": true,
  "name": "new-snazzah",
  "version": "1.0.0",
  "scripts": {
    "build": "preact build && npm run build:fix",
    "build:fix": "npx ncp build/assets build && npx rimraf build/assets",
    "build:clean": "npx rimraf build",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "dependencies": {
    "preact": "^10.5.15",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "@iconify-icons/cib": "^1.1.0",
    "@iconify-icons/mdi": "^1.1.39",
    "@iconify-icons/simple-icons": "^1.1.50",
    "@iconify/react": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "clsx": "^1.1.1",
    "eslint": "^7.32.0",
    "eslint-config-preact": "^1.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "preact-cli": "^3.3.1",
    "preact-purge-tailwindcss": "tim-hub/preact-purge-tailwindcss#master",
    "prettier": "^2.4.1",
    "sirv-cli": "^1.0.14",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "typescript": "^4.4.4"
  }
}
